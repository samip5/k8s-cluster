{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
  {
    "description": "Immich images",
    "groupName": "Immich",
    "matchPackagePatterns": ["immich"],
    "matchDatasources": ["docker"],
    allowedVersions: "/v.*_(.*)/",
    "group": {
      "commitMessageTopic": "{{{groupName}}} group"
    },
    "separateMinorPatch": true
  }]
}
